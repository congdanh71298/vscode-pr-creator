{
  "name": "vscode-pr-creator",
  "displayName": "VSCode PR Creator",
  "description": "Create PR lightening fast",
  "version": "0.0.1",
  "publisher": "Danh Nguyen",
  "author": {
      "name": "Danh Nguyen",
      "email": "congdanh71298@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/congdanh71298/vscode-pr-creator/blob/main/README.md",
  "repository": {
      "type": "git",
      "url": "https://github.com/congdanh71298/vscode-pr-creator"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-pr-creator.createPR",
        "title": "Create PR",
        "icon": "$(rocket)"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "vscode-pr-creator.createPR",
          "group": "navigation@1",
          "when": "scmProvider == git"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
